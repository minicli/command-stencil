{
    "name": "minicli/command-stencil",
    "type": "library",
    "description": "A Minicli command to generate files based on minicli/stencil templates",
    "license": "MIT",
    "homepage": "https://github.com/minicli/command-stencil",
    "keywords": ["cli","command-line", "template", "minicli"],
    "autoload": {
        "psr-4": {
            "Minicli\\": "Command/"
        }
    },
    "require": {
        "minicli/minicli": "^3.1",
        "minicli/stencil": "^0.1.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.21"
    },
    "scripts": {
        "test" : ["pest"]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
